<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	<package name="apart" namespace="/" extends="default">

		<action name="addApartment" class="com.apartment.apart.action.ApartmentAction"
			method="addApartment">
			<result type="json">
				<param name="root">result</param>
			</result>
		</action>

		<action name="readApartment" class="com.apartment.apart.action.ApartmentAction"
			method="readApartment">
			<result type="json">
				<param name="root">result</param>
			</result>
		</action>

		<action name="updateApartment" class="com.apartment.apart.action.ApartmentAction"
			method="updateApartment">
			<result type="json">
				<param name="root">result</param>
			</result>
		</action>

	</package>
</struts>    
